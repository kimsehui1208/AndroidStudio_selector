<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"> //버튼을 눌렀을 때
        <shape>
            <solid android:color="#536DFE"/>
            <corners android:radius="25dp"/>
        </shape>
    </item>

    <item android:state_pressed="false"> //현재 버튼을 안눌렀을 때
        <shape>
            <solid android:color="#69F0AE"/>
            <corners android:radius="25dp"/>
        </shape>
    </item>

</selector>